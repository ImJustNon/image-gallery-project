<main class="mt-5">
    <div class="container">
        <!-- Gallery -->
        <div class="row">
                <% for(let i = 0; i < data.images.length; i++){ %>
                    <div class="col-lg-4 col-md-12 mb-4 mb-lg-0">
                        <a href="<%= data.images[i].fileurl %>" target="_blank">
                            <img src="<%= data.images[i].fileurl %>" class="w-100 shadow-1-strong rounded mb-4" />
                        </a>
                    </div>
                <% } %>
        </div>
        <!-- Gallery -->
    </div>
</main>